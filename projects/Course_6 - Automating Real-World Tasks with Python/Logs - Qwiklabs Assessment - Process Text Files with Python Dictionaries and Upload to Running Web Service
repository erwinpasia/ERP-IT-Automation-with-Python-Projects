#!/usr/bin/env python3

import os
import requests

# Define the directory containing feedback files
feedback_directory = "/data/feedback/"

# Define the format of feedback content
content_format = ["title", "name", "date", "feedback"]

# Define the base URL for the company's website
base_url = "http://34.132.126.180/feedback/"

def read_feedback_file(file_path):
    """
    Read a feedback file and return its content as a dictionary.
    """
    content = {}
    with open(file_path, 'r') as txtfile:
        for i, line in enumerate(txtfile):
            line = line.strip()
            content[content_format[i]] = line
    return content

def post_feedback_to_website(content):
    """
    Post feedback content to the company's website.
    """
    response = requests.post(base_url, json=content)
    if not response.ok:
        raise Exception("POST failed! | Status code: {} | Content: {}".format(response.status_code, content))
    print("Feedback added!")

def main():
    # Iterate through each file in the feedback directory
    for file_name in os.listdir(feedback_directory):
        if file_name.endswith('.txt'):
            file_path = os.path.join(feedback_directory, file_name)
            feedback_content = read_feedback_file(file_path)
            post_feedback_to_website(feedback_content)

if __name__ == "__main__":
    main()
