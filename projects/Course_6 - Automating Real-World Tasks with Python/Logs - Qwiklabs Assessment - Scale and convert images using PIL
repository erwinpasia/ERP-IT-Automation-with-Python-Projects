student-01-fb88deab09b2@linux-instance:~$ cat script_PIL.py
#!/usr/bin/env python3

from PIL import Image
import os

# Define input and output directories
input_directory = 'images'
output_directory = '/opt/icons'

# Ensure the output directory exists
os.makedirs(output_directory, exist_ok=True)

# Iterate through each file in the folder
for filename in os.listdir(input_directory):
    if filename.startswith('ic_') and filename != '.DS_Store':
        file_path = os.path.join(input_directory, filename)
        with Image.open(file_path) as image:
            # Perform image transformations
            image = image.rotate(270).resize((128, 128)).convert('RGB')

            # Get the filename without extension
            file_name_without_extension = os.path.splitext(filename)[0]

            # Save the image in .jpeg format to the output directory
            output_path = os.path.join(output_directory, f'{file_name_without_extension}.jpeg')
            image.save(output_path, 'JPEG')

print('OK')
